{
  "name": "@eramux/rekey",
  "version": "0.1.3",
  "description": "A simple and performant zero dependency library that allows you to modify your object structure using references.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eramux/rekey.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Daniel Schneider",
    "email": "daniel.schneider@eramux.com"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "build:badges": "istanbul-badges-readme",
    "test": "jest"
  },
  "keywords": [
    "rename",
    "js",
    "object",
    "key",
    "array"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && istanbul-badges-readme && git add 'README.md'"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/node": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@commitlint/config-conventional": "9.1.2",
    "@types/jest": "26.0.24",
    "@types/node": "18.11.9",
    "commitlint": "9.1.2",
    "esm": "3.2.25",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "nyc": "15.1.0",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.8",
    "typescript": "4.8.4",
    "istanbul-badges-readme": "1.8.2"
  }
}